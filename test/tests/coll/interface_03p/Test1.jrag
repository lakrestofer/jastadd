import java.util.HashSet;

aspect Test {
	interface I {
		public HashSet<I> targets();
	}
	A implements I;

	public HashSet<I> A.targets() {
	    return new HashSet<>();
	}

	coll HashSet<I> I.set() [new HashSet<>()] with add;
	ASTNode contributes (I)this
		when (this instanceof I)
		to I.set()
		for each ((I)this).targets();
}
