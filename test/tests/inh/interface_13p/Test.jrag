// No conflict with linear inheritance order for inh definition
// .grammar: { S ::= E; E; }
// .result: EXEC_PASS
//    ┌───┐
//    │ A │ getChild.foo = 0
//    ├───┤
// ┌─►│ B │ getChild.foo = 1
// │  ├───┤
// │  │ C │
// │  ├───┤
// └──┤ S │
//    └─┬─┘
//      │  
//    ┌─┴─┐
//    │ E │ inh foo  [E.foo = 1]
//    └───┘
import static runtime.Test.*;
aspect Test13 {
    interface A { }
    interface B extends A { }
    interface C extends B { }

    inh int E.foo();
    eq A.getChild().foo() = 0;
    eq B.getChild().foo() = 1;

    S implements B;
    S implements C;

    public class Test{
	public static void main(String[] args) {
            E e = new E();
            S s = new S(e);
	    testEqual(1, e.foo());
	}
    }
}
